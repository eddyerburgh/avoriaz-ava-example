{
  "name": "avoriaz-ava-example",
  "version": "1.0.0",
  "description": "example project using avoriaz with ava-webpack to test Vue.js components",
  "main": ".eslintrc.js",
  "scripts": {
    "lint:fix": "eslint src test --ext .js,.vue -- --fix",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/ava test/*.spec.js",
    "report": "./node_modules/.bin/nyc report --reporter=html",
    "codecov": "./node_modules/.bin/nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "nyc": {
    "exclude": [
      "node_modules"
    ],
    "extension": [
      ".js",
      ".vue"
    ]
  },
  "ava": {
    "require": [
      "./test/.setup.js"
    ]
  },
  "author": "Edd Yerburgh",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.20.0",
    "avoriaz": "^2.4.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "browser-env": "^2.0.30",
    "codecov": "^2.2.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "nyc": "^11.0.2",
    "require-extension-hooks": "^0.2.0",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^0.3.1",
    "vue-loader": "^11.3.4",
    "vue-template-compiler": "^2.2.6",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "vue": "^2.2.6"
  }
}
